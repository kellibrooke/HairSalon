@{
  Layout = "_Layout";
}
<div class="container">
  <div class="well">
    <h3>Add A New Stylist</h3>
    <form action="/stylists/new" method="post">
      <div class="form-group">
        <label for="stylistName">Stylist Name</label>
        <input type="text" class="form-control" id="stylistName" name="stylistName">
      </div>
      <button class="btn btn-info" type="submit">Add Stylist</button>
    </form>
  </div>
</div>
